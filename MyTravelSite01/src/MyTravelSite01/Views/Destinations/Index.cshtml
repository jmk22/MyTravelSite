@{
}
@model IEnumerable<MyTravelSite01.Models.Destination>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Destinations Index</title>
</head>
<body>
    <h2>Where I've been</h2>
    <h4>@Html.DisplayNameFor(m => m.Name)</h4>
    <ul>
        @foreach(var destination in Model)
        {
            <li>@Html.DisplayFor(modelItem => destination.Name) | @Html.ActionLink("Details", "Details", new { id = destination.Id })</li>

        }
    </ul>
</body>
</html>